    "sass-loader": "12.1.0",
    "sinon": "11.1.1",
    "terser-webpack-plugin": "5.1.3",
    "ts-jest": "27.0.3",
    "ts-loader": "9.2.3",
    "typescript": "4.3.4",
    "url-loader": "4.1.1",
    "vue-jest": "3.0.7",
    "vue-loader": "15.9.7",
    "vue-template-compiler": "2.6.12",
    "wait-on": "5.3.0",
    "webpack": "5.39.1",
    "webpack-bundle-analyzer": "4.4.2",
    "webpack-cli": "4.7.2",
    "webpack-dev-server": "3.11.2",
